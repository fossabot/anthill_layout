  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn')===-1) throw new Error('You must use Yarn to install, not NPM')\"",
    "postinstall": "",
    "clean": "yarn autoclean --force",
    "prebuild": "yarn run clean",
    "build": "./node_modules/.bin/babel app/javascript/packs -d ./dist",
    "pretest": "",
    "jasmine-init": "node ./node_modules/jasmine/bin/jasmine.js init",
    "test": "./node_modules/.bin/babel-node ./node_modules/.bin/jasmine --random=false --config=test/unit/jasmine.json",
    "precoverage": "",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/nyc npm test",
    "postcoverage": ""
  },
  "nyc": {
    "require": [
      "@babel/register"
    ]
  },
  "browserify-css": {
    "autoInject": true,
    "minify": true,
    "rootDir": "."
  },
  "postcss": {
    "parser": "postcss-scss",
    "map": false,
    "plugins": {
      "postcss-plugin": {}
    }
  },
  "browserify": {
    "transform": [
